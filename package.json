{
  "name": "fwestling-site",
  "private": true,
  "version": "0.4.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "pnpm dlx @biomejs/biome lint ./src ",
    "lint:files": "pnpm dlx @biomejs/biome lint ./src 2>&1 | egrep '\\.(ts|tsx):' ",
    "lint:fix": "pnpm dlx @biomejs/biome lint ./src --apply",
    "format": "pnpm dlx @biomejs/biome format ./src --write",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "coverage:ui": "vitest --open --ui --coverage.enabled=true",
    "generate-routes": "npx tsr generate",
    "preview": "vite preview",
    "dummy-release": "echo \"New tag:\" && exit 0",
    "postinstall": "pnpm lefthook install"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@changesets/cli": "^2.27.1",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@fontsource/inter": "^5.0.17",
    "@fontsource/montserrat": "^5.0.17",
    "@fontsource/open-sans": "^5.0.26",
    "@tanstack/react-router": "^1.19.1",
    "@tanstack/router-cli": "^1.19.0",
    "@tanstack/router-devtools": "^1.19.1",
    "firebase": "^10.9.0",
    "framer-motion": "^11.0.8",
    "react": "^18.2.0",
    "react-blurhash": "^0.3.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-photo-album": "^2.3.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "ts-node": "^10.9.2",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.1",
    "yet-another-react-lightbox": "^3.17.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@tanstack/router-vite-plugin": "^1.19.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-istanbul": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "happy-dom": "^13.6.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transformer-svg": "^2.0.2",
    "jsdom": "^24.0.0",
    "lefthook": "^1.6.4",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  }
}
