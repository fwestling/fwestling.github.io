{
  "name": "fwestling-site",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "pnpm dlx @biomejs/biome lint ./src ",
    "lint:files": "pnpm dlx @biomejs/biome lint ./src 2>&1 | egrep '\\.(ts|tsx):' ",
    "lint:fix": "pnpm dlx @biomejs/biome lint ./src --apply",
    "format": "pnpm dlx @biomejs/biome format ./src --write",
    "test": "jest",
    "coverage": "pnpm run test --coverage --watchAll --collectCoverageFrom=src/**/*.{ts,tsx}",
    "preview": "vite preview",
    "dummy-release": "echo \"New tag:\" && exit 0"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "ts-node": "^10.9.2",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "ts-jest": "^29.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest-transformer-svg": "^2.0.2",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  }
}
